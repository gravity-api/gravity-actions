{
  "cliArguments": {
    "source": {
      "description": "The source [x,y] coordinates or element locator value to swipe from."
    },
    "target": {
      "description": "The target [x,y] coordinates or element locator value to swipe to."
    }
  },
  "description": "Swipes the screen by a given coordinates or elements, using a long-press. This action supports swiping element to element, element to coordinates and vice versa.",
  "examples": [
    {
      "description": "Swipes the screen from [x,y-position:100,100] to [x,y-position: 200,200].",
      "literalExample": "swipe {{$ --source:100,100 --target:200,200}}",
      "actionExample": {
        "action": "Swipe",
        "argument": "{{$ --source:100,100 --target:200,200}}"
      }
    },
    {
      "description": "Swipes the element [//button] to [x,y-position:200,200].",
      "literalExample": "swipe {{$ --source://button --target:200,200}}",
      "actionExample": {
        "action": "Swipe",
        "argument": "{{$ --source://button --target:200,200}}"
      }
    },
    {
      "description": "Swipes the element [//button] to [x,y-position:200,200].",
      "literalExample": "swipe {{$ --target:200,200}}",
      "actionExample": {
        "action": "Swipe",
        "onElement": "//button",
        "argument": "{{$ --target:200,200}}"
      }
    },
    {
      "description": "Swipes the element [//button] onto element [//div].",
      "literalExample": "swipe {{$ --source://button --target://div}}",
      "actionExample": {
        "action": "Swipe",
        "argument": "{{$ --source://button --target://div}}"
      }
    },
    {
      "description": "Swipes from [x,y-position:200,200] onto element [//div]",
      "literalExample": "swipe {{$ --source:200,200 --target://div}}",
      "actionExample": {
        "action": "Swipe",
        "argument": "{{$ --source:200,200 --target://div}}"
      }
    },
    {
      "description": "Swipes from [x,y-position:200,200] onto element [//div]",
      "literalExample": "swipe {{$ --source:200,200}}",
      "actionExample": {
        "action": "Swipe",
        "onElement": "//div",
        "argument": "{{$ --source:200,200}}"
      }
    }
  ],
  "name": "Swipe",
  "pluginType": "Action",
  "properties": {
    "locator": "The locator type by which the element will be found.",
    "argument": "The CLI information for this swipe action."
  },
  "protocol": {
    "endpoint": "none",
    "w3c": "https://www.w3.org/TR/webdriver/#actions"
  },
  "scope": [ "mobile-native" ],
  "summary": "Swipes the screen by a given coordinates or elements."
}