{
  "cliArguments": {
    "keys": {
      "description": "Keys to send to the alert.",
      "values": []
    },
    "user": {
      "description": "User name in an alert prompting for credentials.",
      "values": []
    },
    "password": {
      "decription": "Password in an alert prompting for credentials.",
      "values": []
    }
  },
  "description": "Switches to the currently active modal dialog for this particular driver instance.",
  "enum": {
    "dismiss": "Dismisses the alert.",
    "accept": "Accepts the alert."
  },
  "examples": [
    {
      "description": "Dismisses the alert.",
      "literalExample": "switch to alert {dismiss}",
      "actionExample": {
        "action": "SwitchToAlert",
        "argument": "dismiss"
      }
    },
    {
      "description": "Accepts the alert.",
      "literalExample": "switch to alert {accept}",
      "actionExample": {
        "action": "SwitchToAlert",
        "argument": "accept"
      }
    },
    {
      "description": "Sends keys to the alert.",
      "literalExample": "switch to alert {{$ --keys:hello}}",
      "actionExample": {
        "action": "SwitchToAlert",
        "argument": "{{$ --keys:hello}}"
      }
    },
    {
      "description": "Sets the user name and password in an alert prompting for credentials.",
      "literalExample": "switch to alert {{$ --user:userName --pass:userPassword}}",
      "actionExample": {
        "action": "SwitchToAlert",
        "argument": "{{$ --user:userName --pass:userPassword}}"
      }
    },
    {
      "description": "Sends keys to the alert & sets the user name and password in an alert prompting for credentials (sends keys first).",
      "literalExample": "switch to alert {{$ --keys:hello --user:userName --pass:userPassword}}",
      "actionExample": {
        "action": "SwitchToAlert",
        "argument": "{{$ --keys:hello --user:userName --pass:userPassword}}"
      }
    }
  ],
  "name": "SwitchToAlert",
  "pluginType": "Action",
  "properties": {
    "argument": "The action that will be taken on the alert dialog."
  },
  "protocol": {
    "endpoint": "/session/{session-id}/alert/{action}",
    "w3c": "https://www.w3.org/TR/webdriver/#user-prompts"
  },
  "scope": [ "web" ],
  "summary": "Switches to the currently active modal dialog for this particular driver instance.",
  "testOn": "https://gravitymvctestapplication.azurewebsites.net/instructor"
}