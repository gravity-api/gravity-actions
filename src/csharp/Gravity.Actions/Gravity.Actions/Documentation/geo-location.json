    "lat": "The desired GEO location latitude.",
    "lon": "The desired GEO location longitude.",
    "alt": "The desired GEO location altitude (optional)."
  },
  "protocol": {
    "endpoint": "/session/{session-id}/location",
    "w3c": "none"
  }
}